gcc -g -Wall -Werror -Wextra -pedantic *.c *.h -o myshell

echo "/bin/pwd" | ./myshell
echo "/bin/pwd" | /bin/sh
echo

echo "hi/" | ./myshell
echo "hi/" | /bin/sh
echo

echo "pwd/" | ./myshell
echo "pwd/" | /bin/sh
echo

echo "echo hiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiii" | ./myshell
echo "echo hiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiiii" | /bin/sh
echo 

echo "/bin/" | ./myshell
echo "/bin/" | /bin/sh
echo

echo "///" | ./myshell
echo "///" | /bin/sh
echo

echo "" | ./myshell
echo "" | /bin/sh
echo

echo "//pwd" | ./myshell
echo "//pwd" | /bin/sh
echo

echo "     /biin/pwd" | ./myshell
ehoc "     /biin/pwd" | /bin/sh
echo

echo "bin/pwd" | ./myshell
echo "bin/pwd" | /bin/sh
echo

echo "exit" | ./myshell
echo "exit" | /bin/sh
echo

echo "env" | ./myshell
echo "env" | /bin/sh
echo

cat lob | ./myshell
cat lob | /bin/sh
echo

cat filedoesnotexist | ./myshell
cat filedoesnotexist | /bin/sh
echo

echo "*" | ./myshell
echo "*" | /bin/sh
echo

echo "&&" | ./myshell
echo "&&" | /bin/sh



